 -o-background-size: cover;\n    background-size: cover;\n    color: #fff;\n    font-family: 'Quicksand', sans-serif;\n    backdrop-filter: blur(2px);\n}\n\n.calculator_container {\n    display: grid;\n    grid-template-rows: repeat(4, 1fr), min-content;\n    grid-gap: .3rem;\n    background-color: #e8c2ca;\n    border: 3px solid #735d78;\n    border-radius: 40px;\n    box-shadow: 3px 3px 6px rgba(0, 0, 0, 0.16), 0 3px 6px rgba(0, 0, 0, 0.23);\n    overflow: hidden;\n    padding-top: 1rem;\n    -webkit-perspective: 1000;\n    perspective: 1000;\n    padding-top: 1.5rem;\n    margin-right: 15%;\n    width: 270px;\n}\n\nh1 {\n    margin: 0 0 .5rem;\n    text-align: center;\n    font-size: 1.5rem;\n}\n\nspan {\n    flex: 1;\n    font-size: .8rem;\n}\n\n.form_control input {\n    margin-top: .1rem;\n    padding: 0.1rem;\n    width: 95%;\n}\n\n.result {\n    display: flex;\n    background-color: #735d78;\n    color: white;\n    height: 70px;\n    flex-direction: row;\n    justify-content: space-around;\n}\n\n.result_detail {\n    display: flex;\n    width: 100%;\n    padding: .3rem 0 .2rem .7rem;\n    flex-direction: column;\n}\n\n.result_detail:first-child {\n    border-right: 2px solid #e8c2ca;\n}\n\n.result_detail:last-child {\n    width: 60%;\n    flex-shrink: 1;\n}\n\n.result_detail:first-child strong {\n    font-size: 2rem;\n}\n\n.result_detail:last-child strong {\n    font-size: 1.3rem;\n}\n\n.total-text {\n    color: white;\n    text-shadow: 1px 1px #3e1f47;\n}\n\n.dollar {\n    color: #f7d1cd;\n    font-size: 1.5rem;\n}\n\nlabel {\n    display: flex;\n    align-items: center;\n    text-shadow: 1px 1px #9d8189;\n}\n\n.dollar-small {\n    font-size: 1rem;\n}\n\n.icon {\n    width: 18px;\n    height: 18px;\n    fill: white;\n    margin-right: 5px;\n}\n\n.input_container {\n    display: grid;\n    font-size: .9rem;\n    grid-template-columns: repeat(2, 130px);\n    grid-template-rows: repeat(2, 60px);\n    grid-gap: 7px;\n    padding: 5px;\n}\n\n#bill {\n    grid-column: 1 / 3;\n    grid-row: 1 / 2;\n    width: 103%;\n}\n\n#tip {\n    grid-column: 1 / 2;\n    grid-row: 2 / -1;\n}\n\n#split {\n    grid-column: 2 / -1;\n    grid-row: 2 / -1;\n}\n\n.input {\n    background-color: #e8c2ca;\n    height: 1.5rem;\n    border: none;\n    border-bottom: 3px solid #b392ac;\n    color: #735d78;\n    font-size: 1rem;\n}\n\n.input:focus {\n    background-color: #f7d1cd;\n    outline: none;\n}\n\ninput::-webkit-outer-spin-button,\ninput::-webkit-inner-spin-button {\n    -webkit-appearance: none;\n    margin: 0;\n}\n\n::placeholder {\n    color: #b392ac;\n}\n\n.btns {\n    display: flex;\n    width: 100%;\n    flex-direction: row;\n    justify-content: space-between;\n}\n\n.btn {\n    width: 150px;\n    height: 60px;\n    justify-self: center;\n    align-self: center;\n    box-shadow: 1px 1px 1px #735d78;\n    text-align: center;\n    color: #e8c2ca;\n    font-size: 1rem;\n    font-weight: 700;\n    text-transform: uppercase;\n    margin-top: 10px;\n    background-color: #3e1f47;\n    border: none;\n    /* border-top: 2px solid #f7d1cd; */\n    box-shadow: 1px 1px 1px #735d78;\n}\n\n.btn:first-child {\n    margin-right: 2px;\n}\n\n.btn:hover {\n    background-color: #e8c2ca;\n    color: #3e1f47;\n    transform: translateZ(.3rem);\n}\n\n.btn:active {\n    background-color: #735d78;\n    color: #e8c2ca;\n    transform: translateZ(-.5rem);\n}\n\n.btn_calc {\n    width: 60%;\n    margin-right: auto;\n}\n\n.btn_clear {\n    width: 40%;\n}\n\n\n.number {\n    display: grid;\n    grid-template-columns: repeat(3, 1fr);\n    grid-template-rows: repeat(4, 40px);\n    background-color: #e8c2ca;\n    font-size: 1.5rem;\n    grid-gap: 2px;\n    /* border-top: 2px solid #f7d1cd; */\n    border-radius: 5px;\n    -webkit-perspective: 1000;\n    perspective: 1000;\n}\n\n.num {\n    display: flex;\n    background-color: #b392ac;\n    /* box-shadow: 1px 1px 1px #735d78; */\n    justify-content: center;\n    align-items: center;\n}\n\n.num:hover {\n    background-color: #f7d1cd;\n    color: #735d78;\n    cursor: pointer;\n}\n\n.num:active {\n    background-color: #735d78;\n    color: white;\n    transform: translateZ(-1rem);\n}"],"names":[],"version":3,"file":"index.HASH_REF_8a6e625a6f078c797be837fbf556d9ac.css.map"}